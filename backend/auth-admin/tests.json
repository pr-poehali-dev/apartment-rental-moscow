{
  "tests": [
    {
      "name": "OPTIONS request returns CORS headers",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200
    },
    {
      "name": "POST without credentials returns 400",
      "method": "POST",
      "path": "/",
      "body": {},
      "expectedStatus": 400
    },
    {
      "name": "POST with invalid credentials returns 401",
      "method": "POST",
      "path": "/",
      "body": {
        "username": "wrong",
        "password": "wrong"
      },
      "expectedStatus": 401
    }
  ]
}
